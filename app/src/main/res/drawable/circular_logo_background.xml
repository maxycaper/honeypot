<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Transparent outer layer for spacing -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <size android:width="140dp" android:height="140dp" />
        </shape>
    </item>
    <!-- Purple circle -->
    <item
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="#9B30FF" />
        </shape>
    </item>
    <!-- Black square in the middle -->
    <item
        android:left="35dp"
        android:top="35dp"
        android:right="35dp"
        android:bottom="35dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#000000" />
            </shape>
        </rotate>
    </item>
</layer-list>
